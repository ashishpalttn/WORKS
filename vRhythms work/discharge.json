const renderDouble = (property, fieldLabel, required, fieldValues, error, editable, index) => {
  return (
    <div key={index} className="flex p-8">
      <TextField
        id={property}
        name={property}
        label={fieldLabel}
        value={fieldValues && fieldValues[property] ? fieldValues[property] : ''}
        fullWidth
        required={required}
        onChange={handleChange}
        onBlur={validateDouble}
        error={error}
        disabled={!editable}
      />
    </div>
  );
}